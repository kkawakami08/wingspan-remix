    "concurrently": "^8.2.2",
    "isbot": "^3.6.8",
    "jotai": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "eslint": "^8.38.0",
    "tailwindcss": "^3.3.5"
  },
  "engines": {
    "node": ">=18.0.0"
